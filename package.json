{
  "name": "currency-converter-app",
  "version": "1.0.0",
  "description": "Currency converter app made using Electron.js",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build":{
