    }
  },
  "keywords": [],
  "author": "Azoy",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4"
  }
}
